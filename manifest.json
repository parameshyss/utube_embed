{
 "manifest_version": 2,
 "name": "uTube embed",
 "description": "Watch YouTube videos in embedded mode without ads",
 "version": "1.0",
 "icons": { "16" : "icon_16.png",
            "48" : "icon_48.png",
            "128": "icon_128.png" },
 "author": [ {
  "name": "Parameshwaran B",
  "email": "parameshyss@gmail.com"
 } ],
